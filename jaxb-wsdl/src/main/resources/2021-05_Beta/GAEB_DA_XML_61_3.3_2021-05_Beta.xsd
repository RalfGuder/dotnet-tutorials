<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.gaeb.de/GAEB_DA_XML/DA61/3.3" targetNamespace="http://www.gaeb.de/GAEB_DA_XML/DA61/3.3" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="GAEB">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="GAEBInfo" type="tgGAEBInfo">
					<xs:annotation>
						<xs:documentation>Informationen zum GAEB-Datenaustausch</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PrjInfo" type="tgPrjInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Informationen zum Projekt</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SpatialHierarchy" type="tgSpatialHierarchy">
					<xs:annotation>
						<xs:documentation>Raumbuch</xs:documentation>
					</xs:annotation>
					<xs:unique name="UniqueID">
						<xs:selector xpath=".//*"/>
						<xs:field xpath="@ID"/>
					</xs:unique>
					<xs:key name="CatalogAlias">
						<xs:selector xpath="SpatialHierarchyInfo/CatalogDescription"/>
						<xs:field xpath="@Alias"/>
					</xs:key>
					<xs:keyref name="CatalogAliasRef1" refer="CatalogAlias">
						<xs:selector xpath=".//CatalogReference"/>
						<xs:field xpath="@CatalogAlias"/>
					</xs:keyref>
					<xs:keyref name="CatalogAliasRef2" refer="CatalogAlias">
						<xs:selector xpath=".//Expression"/>
						<xs:field xpath="@CatalogAlias"/>
					</xs:keyref>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tgSpatialHierarchy">
		<xs:annotation>
			<xs:documentation>Raumbuch</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SpatialHierarchyInfo">
				<xs:annotation>
					<xs:documentation>Kopfdaten zum Raumbuch</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Topology" maxOccurs="10">
							<xs:annotation>
								<xs:documentation>Gliederungsebene</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Group" use="required">
									<xs:simpleType>
										<xs:restriction base="tgNormalizedString">
											<xs:enumeration value="Site"/>
											<xs:enumeration value="Building"/>
											<xs:enumeration value="BuildingStorey"/>
											<xs:enumeration value="Space"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="CompositionType" use="required">
									<xs:simpleType>
										<xs:restriction base="tgNormalizedString">
											<xs:enumeration value="Complex"/>
											<xs:enumeration value="Element"/>
											<xs:enumeration value="Partial"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="CatalogDescription" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Metadaten für Kataloge, auf die in der Raumbuchinstanz (Body) verwiesen werden kann</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element name="Description" type="tgDescription">
										<xs:annotation>
											<xs:documentation>mehrzeilige Bezeichnung des Kataloges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="Alias" type="tgNormalizedString20" use="required">
									<xs:annotation>
										<xs:documentation>Kürzel für Katalog,  innerhalb des Xml Dokumentes</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="CatalogType" use="required">
									<xs:annotation>
										<xs:documentation>Enumeration</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="tgNormalizedString">
											<xs:enumeration value="SpatialHierarchy"/>
											<xs:enumeration value="DBD-KE"/>
											<xs:enumeration value="DBD-KK"/>
											<xs:enumeration value="STLB-Bau"/>
											<xs:enumeration value="DIN 276-1"/>
											<xs:enumeration value="DIN 277-2"/>
											<xs:enumeration value="Z-Merkmal"/>
											<xs:enumeration value="Z-Artikel"/>
											<xs:enumeration value="UserDefined"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="ID" type="tgNormalizedString" use="required">
									<xs:annotation>
										<xs:documentation>Global ID</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Code" type="tgNormalizedString20" use="required">
									<xs:annotation>
										<xs:documentation>Von Benutzer lesbar</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Name" type="tgNormalizedString60" use="required">
									<xs:annotation>
										<xs:documentation>Von Benutzer eingegeben, nicht zwingend eindeutig auf Hierarchieebene</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Body">
				<xs:annotation>
					<xs:documentation>Instanzdaten zum Raumbuch</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TopologyElement" type="tgTopologyElement" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Ebene</xs:documentation>
							</xs:annotation>
							<xs:unique name="UniqueEbeneCode2">
								<xs:selector xpath="TopologyElement"/>
								<xs:field xpath="@Code"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
				<xs:unique name="UniqueEbeneCode1">
					<xs:selector xpath="TopologyElement"/>
					<xs:field xpath="@Code"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
