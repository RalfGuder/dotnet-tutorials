<definitions
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:wsp="http://www.w3.org/ns/ws-policy"
	xmlns:wsp1_2="http://schemas.xmlsoap.org/ws/2004/09/policy"
	xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://server.ws.gudchensoft.de/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.xmlsoap.org/wsdl/"
	targetNamespace="http://server.ws.gudchensoft.de/"
	name="ServerImplService">
	<types>
		<xsd:schema>
			<xsd:import namespace="http://server.ws.gudchensoft.de/"
				schemaLocation="http://localhost:4434/miniwebservice?xsd=1" />
		</xsd:schema>
	</types>
	<message name="hallo">
		<part name="parameters" element="tns:hallo" />
	</message>
	<message name="halloResponse">
		<part name="parameters" element="tns:halloResponse" />
	</message>
	<portType name="Server">
		<operation name="hallo">
			<input
				wsam:Action="http://server.ws.gudchensoft.de/Server/halloRequest"
				message="tns:hallo" />
			<output
				wsam:Action="http://server.ws.gudchensoft.de/Server/halloResponse"
				message="tns:halloResponse" />
		</operation>
	</portType>
	<binding name="ServerImplPortBinding" type="tns:Server">
		<soap:binding
			transport="http://schemas.xmlsoap.org/soap/http" style="document" />
		<operation name="hallo">
			<soap:operation soapAction="" />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
		</operation>
	</binding>
	<service name="ServerImplService">
		<port name="ServerImplPort" binding="tns:ServerImplPortBinding">
			<soap:address
				location="http://localhost:4434/miniwebservice" />
		</port>
	</service>
</definitions>